{
    "name": "thinqshopping/platform",
    "description": "ThinQShopping E-commerce Platform",
    "type": "project",
    "require": {
        "php": "^8.1",
        "phpmailer/phpmailer": "^6.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "ThinQShopping\\": "src/"
        },
        "files": [
            "includes/functions.php",
            "config/database.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
